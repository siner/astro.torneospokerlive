---
const { circuito } = Astro.props
import { getEventosByCircuito } from '../../lib/api'
import RowEventoV2 from './RowEventoV2'
const events = await getEventosByCircuito(circuito.id)
---

{
    events.map((event) => (
        <RowEventoV2 key={event.id} evento={event} showtour={false} />
    ))
}
