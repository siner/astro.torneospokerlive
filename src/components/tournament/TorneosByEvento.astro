---
import { getEvento, getTorneosByEvento } from '../../lib/api'
import RowTournamentV2 from '../../components/tournament/RowTournamentV2'

const { evento } = Astro.props
const { datamapped: torneos, meta } = await getTorneosByEvento(evento.id)
---

{
    torneos.map((torneo) => (
        <RowTournamentV2 key={torneo.id} torneo={torneo} casino={false} />
    ))
}
