---
import { getTodayTorneos } from '../../lib/api'
import RowTournament from './RowTournament'

const { datamapped: torneos, meta } = await getTodayTorneos()
---

{
    torneos?.map((torneo) => (
        <RowTournament
            key={torneo.id}
            torneo={torneo}
            casino="true"
            event="true"
        />
    ))
}
